/* pages/laundryOrder/payMent/resultMsg/index.wxss */
.result_box{
    width: 90%;
    margin: 50rpx auto;
    text-align: center;
    background-color: #fff;
    border-radius:10rpx; 
    padding: 30rpx 0;
}
.iconfont{
    font-size: 100px;
}
.icon-chenggong{
      color: #3984ff;
}
.icon-shibai{
      color: #eb8c6f;
}
.result_box .p1{
    font-weight: 700;
    font-size: 34rpx;
    margin: 40rpx 0;
}
.result_box .p2{
    color: #ec7868;
}
.order_but navigator{
    width: 80%;
    background-color: #fff;
    margin: 0 auto;
    margin-bottom: 30rpx;
    text-align: center;
    line-height: 80rpx;
    border: 1px solid #FF4949;
    color: #FF4949;
    border-radius: 80rpx;
}
.backg-grad {
background-image:linear-gradient(45deg, #FF8687, #FF4949) !important; 
} 
.order_but navigator.active{
    background-color: #3984ff;
    color: #fff;
}
.ic_success,.ic_fail{
width: 300rpx;
height: 300rpx;
}
