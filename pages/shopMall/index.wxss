.slideDown{
   animation: 1s fadeIn
}
.slideUp{
    height: 0;
    padding: 0;
    overflow: hidden;
    transform: scale(0.8,0.8);
     transition: .5s  ease;
} 
@keyframes fadeIn {
0% {
opacity: 0; /*初始状态 透明度为0*/
}
50% {
opacity: 0.5; /*中间状态 透明度为0*/
}
100% {
opacity: 1; /*结尾状态 透明度为1*/
}
}

@keyframes fadeOut {
0% {
opacity: 1; /*初始状态 透明度为0*/
}
50% {
opacity: 0.5; /*中间状态 透明度为0*/
}
100% {
opacity: 0; /*结尾状态 透明度为1*/
}
}
