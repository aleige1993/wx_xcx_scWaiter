/* pages/usCenter/setUser/index.wxss */
page{
  background-color: #f2f2f2;
}
.cell_box{
  background-color: #fff;
  margin-top: 30rpx;
}
.cell_box .cell_list{
  border-bottom:1px solid #eee;
  padding:20rpx 30rpx;
  display: flex;
  justify-content:space-between; 
}
.cell_box .cell_list view:last-child{
  flex: 1;
  text-align: right;
}
.cell_box .cell_list  view:first-child{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.cell_box .cell_list view image{
  width:50rpx;
  height: 50rpx;
  border-radius:100%; 
}