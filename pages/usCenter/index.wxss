/* pages/usCenter/userInfo.wxss */
page{
    background-color: #f2f2f2;
}
.bg_img{
    width: 100%;
    height: 260rpx;
}
.info_model{
    position: relative;
}
.flut{
    position: absolute;
    top:120rpx;
    width: 92%; 
    z-index: 5;
    left: 4%;
}
.user_info{
    background-color: #fff;
    border-radius:10rpx; 
    display: flex;
    padding: 40rpx;
    position: relative;
    /* overflow: hidden; */
    margin-bottom: 40rpx;
}
.vip_info{
    position: absolute;
    right: 0rpx;
    bottom: -7rpx;
    z-index: 6;
}
.vip_info image{
    width: 175rpx;
    height: 100rpx;
}
.user_info image.aouter_image{
    width:100rpx;
    height: 100rpx;
    border-radius:6rpx; 
    margin-right: 30rpx;
}
.user_name{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-content: space-around;
}
.model_info{
    margin-top: 20rpx;
    background-color: #fff;
    border-radius:10rpx; 
}
.heder_info{
    padding: 20rpx;
    border-bottom: 1px solid #eee;
    display: flex;
    justify-content: space-between;
}
.coupon_panl{
    display: flex;
    justify-content: space-around;
    padding: 20rpx 0;
}
.coupon_panl navigator{
    text-align: center;
}
.coupon_panl navigator view{
    color: #888;
    font-size: 24rpx;
}
.coupon_panl navigator icon{
    font-size:24px; 
    margin-bottom: 4rpx;
}
.xufei{
    padding: 0rpx 20rpx;
    border:1px solid #ea857d;
    color: #ea857d;
    border-radius:30rpx; 
    margin-left: 20rpx;
}

.shadow-just-bottom{
    box-shadow:0rpx 8rpx 8rpx #e2e2e2;
}
.shadow-just-top{
    box-shadow:0rpx  -20rpx  50rpx  0rpx rgb(176, 147, 245);
}
.icon-gengyishi{
    font-size: 30px !important;
}
.laber_text{
    margin-top: 10rpx;
}
.tiem_text{
    font-size:24rpx;
    color: #666;
}