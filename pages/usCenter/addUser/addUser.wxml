<!--pages/usCenter/addUser/addUser.wxml-->
<view>
  <view class='user_up'>
    <image src='../../../static/images/error.png'></image>
    <view>点击头像上传</view>
  </view>
  <view>
    <van-cell-group>
      <van-field
        value="{{ username }}"
        label="姓名"
        placeholder="请输入姓名"
      />
      <van-field
        value="{{ phone }}"
        label="手机号"
        placeholder="请输入手机号"
      />
      <van-field
        value="{{ pwsd }}"
        label="密码"
        placeholder="请输入密码"
      />
    </van-cell-group>
  </view>
  <view class='marig-primary  fixed-bottom'>
      <van-button size="normal" type="primary" custom-class="primary">确定</van-button>
    </view>
</view>
