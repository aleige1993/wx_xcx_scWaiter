/* pages/usCenter/couponInfo/index.wxss */
page{
    background-color: #f2f2f2;
}
.tab_panl{
    position: fixed;
    width: 100%;
    z-index: 100;
    left: 0;
    top: 0;
}
.scorll_view{
    width: 100%;
    margin-top: 120rpx;
    padding-bottom: 40rpx;
}
.coupon_model{
    width: 94%;
    margin: 30rpx auto 0 auto;
    display: flex;
}
.cou_bg{
    width: 35%;
    /* background-color: #666; */
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    align-content: center;
    color: #fff;
    background-size: 100% 100%;
}
.cou_bg label{
    font-size: 54rpx;
    font-weight: 600;
}
.cou_bg text{
    font-size: 24rpx;
}
.cou_title{
    flex: 1;
    padding: 20rpx;
    display: flex;
    justify-content: space-between;
    background-color: #fff;
    position: relative;
    overflow: hidden;
    background-size: 30% 60%;
    background-repeat:no-repeat;
    background-position: 320rpx 50rpx;
}
.title_conter view{
    margin-bottom: 10rpx;
}
.title_conter view:last-child{
    margin-bottom: 0;
}
.title_conter  .name_text{
    font-weight: 600;
    color: #FF5D9E;
    font-size: 34rpx;
}
.name_text.text_violet{
     color: #8f71ff;
}
.name_text.text_pink{
     color: #FF5D9E;
}
.name_text.text_blue{
     color: #82acff;
}
.name_text.text_blue{
     color: #82acff;
}
.text_gray{
     color: #cccccc !important;
}

.title_conter  .type_text{
    font-weight: 600;
    color: #414141;
}
.title_conter  .p_text{
    color: #999;
}
.icon_right{
    display: flex;
    justify-content: center;
    align-content: center;
    align-items: center;
}
.icon_right icon.icon-you1{
    color: #999 !important;
}
.tag_text{
    position: absolute;
    right:-26rpx;
    top: 20rpx;
    text-align: center;
    font-size: 20rpx;
    color: #fff; 
    transform: rotateZ(45deg);
    width: 125rpx;
    padding: 4rpx 0;
}
.bg_red{
 background-color: #e51a1a;
}
.bg_yeller{
     background-color: #f2af0d;
}

.bg_violet{
    background-image: url('data:image/png;base64,')
}
.bg_pink{
    background-image: url('data:image/png;base64,')
}
.bg_blue{
 background-image: url('data:image/png;base64,')
}
.bg_gray{
