    padding: 20rpx 30rpx;
}
.order_title{
    display: flex;
    justify-content: space-between;
}
.order_title .exName text:last-child{
    font-size: 12px;
    color: #999;
    margin-left: 10rpx;
}

.order_detail .v-li{
    display: flex;
    margin-top: 20rpx;
    color: #777;
    justify-content: space-between;
    align-items: center;
}